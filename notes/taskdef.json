{
  "containerDefinitions": [
    {
      "cpu": 256,
      "essential": true,
      "image": "###IMAGE###",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "hw-ecr",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "hello"
        }
      },
      "memory": 512,
      "name": "notes",
      "portMappings": [
        {
          "containerPort": 8120,
          "hostPort": 8120,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "cpu": "256",
  "executionRoleArn": "###EXECUTION_ROLE_ARN###",
  "family": "###FAMILY###",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "taskRoleArn": "###TASK_ROLE_ARN###"
}
